{
    "name": "framerate",
    "main": "expo-router/entry",
    "version": "0.0.0",
    "scripts": {
        "start": "expo start",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "build:ios": "npm ci && expo prebuild --platform=ios && cd ios && pod install && cd ..",
        "deploy:ios": "eas build --platform ios --profile production --auto-submit",
        "builddev:ios": "eas build --platform ios --profile development",
        "build:android": "npm ci && expo prebuild --platform=android",
        "lint": "biome check",
        "typecheck": "tsc --noEmit",
        "format": "biome format --write .",
        "depcruise:modules": "depcruise modules --include-only '^modules' --collapse 2 --output-type dot | dot -T svg > docs/module-dependency-graph.svg",
        "openapi:update": "curl http://localhost:3000/api-docs/openapi.json > services/openapi.json",
        "openapi:generate": "rm -rf services/framerateBackend && openapi-generator-cli generate --generator-key typescript-fetch"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@expo/vector-icons": "^15.0.2",
        "@react-native-async-storage/async-storage": "2.2.0",
        "@react-native-community/datetimepicker": "8.4.4",
        "@react-native-community/netinfo": "11.4.1",
        "@react-native-segmented-control/segmented-control": "2.5.7",
        "@react-navigation/native": "^7.0.0",
        "@reillymc/es-utils": "^0.0.4",
        "@reillymc/react-native-components": "~0.8.0",
        "@shopify/react-native-skia": "2.2.12",
        "@tanstack/query-async-storage-persister": "^5.81.2",
        "@tanstack/react-query": "^5.81.2",
        "@tanstack/react-query-persist-client": "^5.81.2",
        "date-fns": "^4.1.0",
        "expo": "^54.0.0",
        "expo-blur": "~15.0.7",
        "expo-clipboard": "~8.0.7",
        "expo-constants": "~18.0.8",
        "expo-dev-client": "~6.0.12",
        "expo-device": "~8.0.7",
        "expo-font": "~14.0.8",
        "expo-image": "~3.0.8",
        "expo-linking": "~8.0.8",
        "expo-router": "~6.0.5",
        "expo-secure-store": "~15.0.7",
        "expo-splash-screen": "~31.0.10",
        "expo-status-bar": "~3.0.8",
        "expo-system-ui": "~6.0.7",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-native": "0.81.4",
        "react-native-gesture-handler": "~2.28.0",
        "react-native-ios-context-menu": "^3.2.1",
        "react-native-reanimated": "~4.1.0",
        "react-native-safe-area-context": "~5.6.0",
        "react-native-screens": "~4.16.0",
        "react-native-svg": "15.12.1",
        "react-native-web": "^0.21.0",
        "react-native-worklets": "0.5.1",
        "victory-native": "^41.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@biomejs/biome": "2.0.4",
        "@openapitools/openapi-generator-cli": "^2.15.3",
        "@types/react": "~19.1.0",
        "dependency-cruiser": "^16.8.0",
        "typescript": "~5.9.2"
    },
    "private": true
}
