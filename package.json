{
    "name": "framerate",
    "main": "index",
    "version": "0.3.12",
    "scripts": {
        "start": "expo start",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "build:ios": "npm ci && expo prebuild --platform=ios && cd ios && pod install && cd ..",
        "build:android": "npm ci && expo prebuild --platform=android",
        "lint": "biome lint",
        "typecheck": "tsc --noEmit",
        "format": "biome format --write .",
        "depcruise:modules": "depcruise modules --include-only '^modules' --collapse 2 --output-type dot | dot -T svg > docs/module-dependency-graph.svg",
        "openapi:update": "curl http://localhost:3000/api-docs/openapi.json > services/openapi.json",
        "openapi:generate": "rm -rf services/framerateBackend && openapi-generator-cli generate --generator-key typescript-fetch"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@expo/vector-icons": "^14.1.0",
        "@react-native-async-storage/async-storage": "2.1.2",
        "@react-native-community/datetimepicker": "8.3.0",
        "@react-native-community/netinfo": "11.4.1",
        "@react-native-segmented-control/segmented-control": "2.5.7",
        "@react-navigation/native": "^7.0.0",
        "@reillymc/react-native-components": "^0.5.15",
        "@shopify/react-native-skia": "^2.0.5",
        "@tanstack/query-async-storage-persister": "^5.81.2",
        "@tanstack/react-query": "^5.81.2",
        "@tanstack/react-query-persist-client": "^5.81.2",
        "date-fns": "^4.1.0",
        "expo": "^53.0.0",
        "expo-blur": "~14.1.5",
        "expo-clipboard": "~7.1.4",
        "expo-constants": "~17.1.6",
        "expo-dev-client": "~5.2.1",
        "expo-device": "~7.1.4",
        "expo-font": "~13.3.1",
        "expo-image": "~2.3.0",
        "expo-linking": "~7.1.5",
        "expo-router": "~5.1.0",
        "expo-secure-store": "~14.2.3",
        "expo-splash-screen": "~0.30.9",
        "expo-status-bar": "~2.2.3",
        "expo-system-ui": "~5.0.9",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-native": "0.79.4",
        "react-native-gesture-handler": "~2.24.0",
        "react-native-ios-context-menu": "^2.5.1",
        "react-native-reanimated": "~3.17.4",
        "react-native-safe-area-context": "5.4.0",
        "react-native-screens": "~4.11.1",
        "react-native-svg": "15.11.2",
        "react-native-web": "^0.20.0",
        "victory-native": "^41.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@biomejs/biome": "^1.9.3",
        "@openapitools/openapi-generator-cli": "^2.15.3",
        "@types/react": "~19.0.10",
        "dependency-cruiser": "^16.8.0",
        "typescript": "~5.8.3"
    },
    "private": true
}
